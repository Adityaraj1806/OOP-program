#include<iostream>

using namespace std;

union student

{

int rollno; float marks;

};

student s1;

int main().

{

s1.rollno=14751;

s1.marks 35.5;

cout<<"ADITYA RAJ SINGH TOMAR"<<endl;

cout<<"23100BTCSE14737"<<endl;

cout<<s1.rollno<<endl;

cout<<s1.marks<<endl;

cout<<"sizeofunion :"<<sizeof(s1)<<endl;

return 0;

}
